{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "sinapsi"
  },
  "apps": [
    {
      "baseHref": "/en",
      "name": "sinapsi",
      "root": "src",
      "outDir": "dist",
      "assets": [{
        "glob": "**/*",
        "input": "theme/assets/",
        "output": "assets/"
      }],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "theme/main.scss"
      ],
      "scripts": [
          "../node_modules/moment/min/moment.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.en.prod.ts",
        "ca": "environments/environment.ca.prod.ts",
        "es": "environments/environment.es.prod.ts",
        "en": "environments/environment.en.prod.ts"
      },
      "stylePreprocessorOptions": {
        "includePaths": [
          "theme"
        ]
      }
    },
    {
      "baseHref": "/en",
      "name": "embed",
      "root": "src",
      "outDir": "dist/embed",
      "deployUrl": "embed",
      "assets": [{
        "glob": "**/*",
        "input": "theme/assets/",
        "output": "assets/"
      }],
      "index": "embed/index.html",
      "main": "embed/main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "theme/main.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.en.prod.ts",
        "ca": "environments/environment.ca.prod.ts",
        "es": "environments/environment.es.prod.ts",
        "en": "environments/environment.en.prod.ts"
      },
      "stylePreprocessorOptions": {
        "includePaths": [
          "theme"
        ]
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    }
  }
}
